<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="com.yiya.collector.dao.ImageDao">
     <select id="queryAllImageBean" parameterType="com.yiya.collector.bean.ImageBean" resultType="com.yiya.collector.bean.UserBean">
         select * from ImageBean
     </select>
     <insert id="insertImageBean" parameterType="com.yiya.collector.bean.ImageBean" flushCache="true">
          INSERT INTO ImageBean (cata_id,title,thumbNail,pageNum,imgPaths,contextHtml,currentPageUrl,baseUrl) VALUES 
            (#{cata_id},#{title},#{thumbNail},#{pageNum},#{imgPaths},#{contextHtml},#{currentPageUrl},#{baseUrl})
     </insert>
     <delete id="deleteImageBean" parameterType="int">
         DELETE FROM ImageBean WHERE id=#{id} 
     </delete>
</mapper>